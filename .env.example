# ===========================================
# OmniBooking Agent - Self-Hosted Configuration
# ===========================================
# Copy this file to .env and fill in your values

# Database
DB_USER=omnibooking
DB_PASSWORD=your-secure-password-here
DB_NAME=omnibooking

# Security
JWT_SECRET=your-super-secret-jwt-key-minimum-32-chars

# Meta Webhooks
WEBHOOK_VERIFY_TOKEN=your-webhook-verify-token

# WhatsApp Cloud API
WHATSAPP_TOKEN=your-whatsapp-token
WHATSAPP_PHONE_NUMBER_ID=your-phone-number-id
WHATSAPP_BUSINESS_ACCOUNT_ID=your-business-account-id

# Instagram Messaging API
INSTAGRAM_PAGE_ACCESS_TOKEN=your-instagram-page-access-token
INSTAGRAM_APP_ID=your-instagram-app-id
INSTAGRAM_APP_SECRET=your-instagram-app-secret

# SimplyBook
SIMPLYBOOK_COMPANY=your-company
SIMPLYBOOK_API_KEY=your-api-key
SIMPLYBOOK_SECRET_KEY=your-secret-key
SIMPLYBOOK_MOCK=false

# Frontend (update with your domain)
VITE_API_BASE_URL=http://localhost:3000
